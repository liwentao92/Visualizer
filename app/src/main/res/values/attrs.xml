<?xml version="1.0" encoding="utf-8"?>
<resources>
    
    <declare-styleable name="MusicView">
        <attr name="itemNum" format="integer"/>
        <attr name="itemWidth" format="dimension"/>
        <attr name="musicColor" format="integer"/>
        <attr name="randColor" format="boolean"/>
        <attr name="musicType" format="integer">
            <enum name="RECT" value="1"></enum>
            <enum name="MRECT" value="2"></enum>
            <enum name="MMRECT" value="3"></enum>
        </attr>
    </declare-styleable>


</resources>